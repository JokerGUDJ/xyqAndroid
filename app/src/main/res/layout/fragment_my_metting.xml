<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <com.xej.xhjy.ui.view.StatusBarHoldView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="@color/white" />

    <RelativeLayout
        android:id="@+id/ll_title_bg"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/head_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@drawable/btn_tran_press_bg"
            android:paddingLeft="12dp"
            android:paddingRight="12dp"
            android:src="@drawable/ic_all_return" />
        <TextView
            android:id="@+id/head_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:layout_centerInParent="true"
            android:maxLines="1"
            android:text="我的会议"
            android:paddingLeft="@dimen/dp_5"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_18" />
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:orientation="horizontal"
        android:background="@color/white">
        <com.xej.xhjy.ui.view.TextviewTobTabView
            android:id="@+id/tb_offline_meeting"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
        <com.xej.xhjy.ui.view.TextviewTobTabView
            android:id="@+id/tb_online_meeting"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#eeeeee"/>
    <FrameLayout
        android:id="@+id/container_fg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingTop="@dimen/dp_15"
        android:background="#FCFCFC">

    </FrameLayout>
</LinearLayout>


